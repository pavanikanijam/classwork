i=1;
j=1;
a=int(input("enter number of rows"))
for i in (1,a+1):
  for j in (1,i+1):
     print(i,end="")
 print()
 
 ////
 palindrome of a given number:
 rev=0
 n=int(input("Enter number"))
 temp=n
 while(n!=0):
   x=n%10
   rev=rev*10+x
   n=n/10
 if(temp==rev):
   print("palindrome")
 else:
 print("Not a palindrome")
 
