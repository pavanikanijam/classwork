n=153
temp=n
while(n>0):
  x=n/10
  rev=rev*rev*rev+x;
  n=n//10
 if(temp==rev):
   print("palindrome")
 else:
   print("Not a palindrome")
   
   
s=int(input("Enter side"))
l=int(input("Enter length"))
b=int(input("Enter breadth"))
h=int(input("Enter height"))
print("area of square:",s*s)
print("area of rectangle:",l*b)
print("area of triangle:",(1/2)*b*h)


